<div class="container-fluid">
	<h1 class="contact_header">Contact Info</h1>
		<div class="contactinfo">
			<div class="col-md-5 col-xs-12">
				<h2>Email</h2>
				<p>kaylabutler.violin@yahoo.com</p>

				<h2>Social Media</h2>
				<ul>
				<li>Facebook</li>
				<li>Instagram</li>
				<li>Tumblr</li>
				<li>Pinterest</li>
				<li>Twitter</li>
				</ul>
			</div>

<%= flash[:notice] %>

<%= form_tag("/static_pages/thank_you") do %>
<div class="row">
	<div class="col-md-5 col-xs-12">
		<div class="form-group">
			<%= label_tag :name %>
			<%= text_field_tag :name, nil, class: "form-control", placeholder: "Enter Name" %>
		</div>
		<div class="form-group">
			<%= label_tag :email, "Email Address" %>
			<%= text_field_tag :email, nil, class: "form-control", placeholder: "Enter Email" %>
		</div>
		<div class="form-group">
			<%= label_tag :message %>
			<%= text_area_tag :message, nil, class: "form-control", placeholder: "Enter Message" %>
			<%= submit_tag "Send" %>
			</div><!-- /contactinfo -->
		</div>
	</div>
</div>
<% end %>
